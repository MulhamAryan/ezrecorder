<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http:// www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>Label</key>
		<string>visca-daemon</string>
		<key>ServiceDescription</key>
		<string>Visca server daemon</string>

		<key>OnDemand</key>
		<false/>
		<key>RunAtLoad</key>
		<true/>

		<key>StandardOutPath</key>
		<string>/Library/Logs/visca-daemon.log</string>
		<key>StandardErrorPath</key>
		<string>/Library/Logs/visca-daemon.log</string>

		<key>ProgramArguments</key>
		<array>
			<string>/usr/local/sbin/visca-daemon.bash</string>
		</array>
	</dict>
</plist>

